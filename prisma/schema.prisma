generator client {
  provider  = "prisma-client-js"
  output    = "../node_modules/.prisma/client"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL")
}

model Event {
  id               Int      @id @default(autoincrement())
  firstName        String
  lastName         String
  eventName        String   @unique
  registrationDate DateTime
  companyName      String
}
